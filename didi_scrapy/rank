<script type="text/javascript">
    //<![CDATA[
    for ( var i=0; i<$("div.tabBox").length; i++){
        var tab = new NTES.ui.Slide( $($("div.tabBox")[i]).$("ul.tabNav > li"), $($("div.tabBox")[i]).$("div.tabContents"), "active", "mouseover");
